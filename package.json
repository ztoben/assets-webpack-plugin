    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0",
    "chai": "4.2.0",
    "css-loader": "4.3.0",
    "mini-css-extract-plugin": "0.11.2",
    "mocha": "8.1.3",
    "rimraf": "3.0.2",
