    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0",
    "chai": "4.2.0",
    "css-loader": "5.0.1",
    "mini-css-extract-plugin": "1.3.3",
    "mocha": "8.2.1",
    "rimraf": "3.0.2",
    "snazzy": "9.0.0",
